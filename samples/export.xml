<?xml version="1.0" encoding="utf-8"?>
<XmlConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <UseDiaSymbolProvider>true</UseDiaSymbolProvider>
    <Types>
        <Type Name="MyTestClass" />
    </Types>
    <Transformations>
        <Transformation OriginalType="std::basic_string&lt;char,${char_traits},${allocator}&gt;" NewType="SharpDebug.CommonUserTypes.NativeTypes.std.@string" Constructor="${new}" />
        <Transformation OriginalType="std::basic_string&lt;wchar_t,${char_traits},${allocator}&gt;" NewType="SharpDebug.CommonUserTypes.NativeTypes.std.wstring" Constructor="${new}" />
        <Transformation OriginalType="std::vector&lt;${T},${allocator}&gt;" NewType="SharpDebug.CommonUserTypes.NativeTypes.std.vector&lt;${T}&gt;" Constructor="${new}" />
        <Transformation OriginalType="std::list&lt;${T},${allocator}&gt;" NewType="SharpDebug.CommonUserTypes.NativeTypes.std.list&lt;${T}&gt;" Constructor="${new}" />
    </Transformations>
</XmlConfig>
